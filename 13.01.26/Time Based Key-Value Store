class TimeMap:
    def __init__(self):
        self.d={}
    def set(self,key,value,t):
        self.d.setdefault(key,[]).append((t,value))
    def get(self,key,t):
        if key not in self.d: return ""
        arr=self.d[key]
        l,r=0,len(arr)-1
        res=""
        while l<=r:
            m=(l+r)//2
            if arr[m][0]<=t:
                res=arr[m][1]; l=m+1
            else:
                r=m-1
        return res
